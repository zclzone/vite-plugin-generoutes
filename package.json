{
  "name": "vite-plugin-generoutes",
  "type": "module",
  "version": "0.2.11",
  "packageManager": "pnpm@10.6.2",
  "description": "A Vite plugin that generate routes based on the file structure, supports dynamic routes, and supports custom meta data for each route.",
  "author": "Ronnie Zhang <zclzone@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/zclzone/vite-plugin-generoutes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zclzone/vite-plugin-generoutes.git"
  },
  "bugs": "https://github.com/zclzone/vite-plugin-generoutes/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks",
    "up:deps": "taze major -I"
  },
  "peerDependencies": {
    "vite": "^6.1.0"
  },
  "dependencies": {
    "@antfu/utils": "^9.1.0",
    "@vue/compiler-sfc": "^3.5.13",
    "chalk": "^5.4.1",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.1",
    "prettier": "^3.5.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.10.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.17.24",
    "bumpp": "^10.1.0",
    "eslint": "^9.22.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.5.0",
    "pnpm": "^10.6.2",
    "rimraf": "^5.0.10",
    "simple-git-hooks": "^2.11.1",
    "taze": "^19.0.2",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "vitest": "^3.0.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
