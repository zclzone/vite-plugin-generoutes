{
  "name": "vite-plugin-generoutes",
  "type": "module",
  "version": "2.0.1",
  "packageManager": "pnpm@10.28.2",
  "description": "A Vite plugin that generate routes based on the file structure, supports dynamic routes, and supports custom meta data for each route.",
  "author": "Ronnie Zhang <zclzone@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/zclzone/vite-plugin-generoutes",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zclzone/vite-plugin-generoutes.git"
  },
  "bugs": "https://github.com/zclzone/vite-plugin-generoutes/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "prepare": "simple-git-hooks",
    "up:deps": "taze major -I"
  },
  "peerDependencies": {
    "vite": ">=6.4.1",
    "vue-router": ">=4.0.0"
  },
  "dependencies": {
    "@antfu/utils": "^9.3.0",
    "fs-extra": "^11.3.3",
    "glob": "^13.0.0",
    "prettier": "^3.8.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@antfu/ni": "^28.2.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^25.2.0",
    "bumpp": "^10.4.0",
    "eslint": "^9.39.2",
    "esno": "^4.8.0",
    "lint-staged": "^16.2.7",
    "pnpm": "^10.28.2",
    "rimraf": "^6.1.2",
    "simple-git-hooks": "^2.13.1",
    "taze": "^19.9.2",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "^4.0.18",
    "vue-router": "^5.0.1"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
